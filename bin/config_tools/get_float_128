#! /usr/bin/env python

import sys
import re
from configlib import getstatusoutput, getoutput

cxx = sys.argv[1]
cxxflags = ""
if len(sys.argv) > 2:
  cxxflags = " ".join(sys.argv[2:])

cmd = "%s %s -o tmp_cpp_version.o -c bin/config_tools/float128.cc" % (cxx, cxxflags)
rc, text = getstatusoutput(cmd)
getoutput("rm -f tmp_cpp_version.o")

if rc == 0:
  print("yes")
else:
  print("no")

